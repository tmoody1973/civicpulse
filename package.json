{
  "name": "hakivo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "worker": "npx tsx workers/brief-worker.ts",
    "worker:news": "npx tsx workers/news-worker.ts",
    "worker:images": "dotenv -e .env.local -- npx tsx workers/image-worker.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "playwright test",
